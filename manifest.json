{
"manifest_version": 2,
    "name": "SkyDome",
    "description": "Replaces 'Rogers Centre' with the true name SkyDome.",
    "version": "0.1",
    "icons": {
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
